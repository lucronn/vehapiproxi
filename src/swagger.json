    {
      "name": "Assets",
      "description": "Operations for retrieving articles, graphics, and asset content"
    },
    {
      "name": "Vehicles",
      "description": "Operations for vehicle information and selection"
    },
    {
      "name": "Search",
      "description": "Operations for searching articles and content"
