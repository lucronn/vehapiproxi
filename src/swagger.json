    "/api/source/{contentSource}/vehicle/{vehicleId}/article/{articleId}": {
      "get": {
        "tags": [
          "Assets"
        ],
        "summary": "Get article by ID",
        "description": "Content and relevant metadata for a specific article in the context of a vehicle",
        "operationId": "getArticleById",
        "parameters": [
          {
            "$ref": "#/components/parameters/ContentSource"
          },
          {
            "$ref": "#/components/parameters/VehicleId"
          },
          {
            "name": "articleId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "The article ID"
          },
          {
            "name": "motorVehicleId",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "Motor vehicle ID"
          },
          {
            "name": "prettyPrint",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean"
            },
            "description": "Pretty print the response"
          },
          {
            "name": "bucketName",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "Bucket name"
          },
          {
            "name": "articleSubtype",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "Article subtype"
          },
          {
            "name": "searchTerm",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "Search term"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ArticleResponse"
                }
              }
            }
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
